<%- include('../layouts/admin', { body: ` <div class="whole-page">
  <nav class="sidebar">
    <div class="image-text">
      <span class="image">
        <img src="/images/icons/cafe.png" alt="Dashboard Icon">
      </span>
      <div class="text">Dashboard</div>
    </div>
    <ul id="js-menu-links" class="nav-links"></ul>
  </nav>
  <div class="main">
    <div class="search-section">
      <div class="title-part">Inventory Management</div>
      <div class="search-part">
        <input class="search-bar" type="text" placeholder="Search">
        <button class="search-button">
          <i class='bx bx-search'></i>
        </button>
      </div>
    </div>
    <div class="inventory">
      <div class="restock-section">
        <table>
          <thead>
            <th>No</th>
            <th>Item Name</th>
            <th>Current Amount</th>
            <th>Restock Amount</th>
            <th>Decrease Amount</th>
            <th>Action</th>
          </thead>
          <tbody id="inventory-table-body">
            <!-- Inventory items will be populated here -->
          </tbody>
        </table>
      </div>
    </div>
  </div>
  </div>
  ` }) %>