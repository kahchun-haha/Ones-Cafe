<%- include('../layouts/main', { body: `     <div class="login-container">
    <div id="admin-login-container">
        <form id="admin-login-form">
        <h2>Login as Admin</h2>
        <div class="form-field">
            <input type="email" id="admin-login-email" name="admin-email" placeholder="Enter your email address" required>
        </div>
        <div class="form-field">
            <input type="password" id="admin-login-password" name="admin-password" placeholder="Password" required>
        </div>
        <div class="links-container">
            <a href="/admin/adminRegister">Register</a>
        </div>
        <div class="btn-container">
            <button type="submit" id="admin-login">Sign in</button>
        </div>
        </form>
    </div>
    ` }) %>